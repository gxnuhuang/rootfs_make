proc		/proc	proc	defaults    0	0
sysfs           /sys    sysfs   defaults    0   0
tmpfs		/tmp	tmpfs	defaults    0   0
/dev/mmcblk1p1 /mnt/storage	auto       rw,sync,noauto  0  0
/dev/mmcblk1p2 /mnt/firmware	auto       rw,sync,noauto  0  0
/dev/mmcblk1p3 /mnt/platform	auto       rw,sync,noauto  0  0
